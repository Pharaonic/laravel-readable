{
    "name": "pharaonic/laravel-readable",
    "description": "Laravel - Readable Number (Integer|decimal) & DateTime & Time Length & File Size.",
    "keywords": [
        "php",
        "laravel",
        "human readable",
        "readable",
        "pharaonic"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Raggi",
            "email": "support@pharaonic.io",
            "homepage": "https://pharaonic.io"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Readable\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pharaonic\\Laravel\\Readable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Readable\\ReadableServiceProvider"
            ],
            "aliases": {
                "Readable": "Pharaonic\\Laravel\\Readable\\Readable"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
